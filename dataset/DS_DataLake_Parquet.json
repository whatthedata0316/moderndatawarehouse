{
	"name": "DS_DataLake_Parquet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_AzureDataLakeStorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ContainerName": {
				"type": "string",
				"defaultValue": "raw"
			},
			"FolderName": {
				"type": "string",
				"defaultValue": "SqlAzure"
			}
		},
		"folder": {
			"name": "wth-team2"
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@dataset().FolderName",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().ContainerName",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
}